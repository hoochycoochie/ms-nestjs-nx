    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "install": "^0.13.0",
    "nestjs-pino": "^4.1.0",
    "npm": "^10.8.0",
    "pg": "^8.11.5",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
