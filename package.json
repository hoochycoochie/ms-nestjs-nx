{
  "name": "@medsen/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "typeorm": "ts-node --project ./tsconfig.base.json ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./database/src/db/portal/datasource/index.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./database/src/db/portal/datasource/index.ts migration:generate ./database/src/db/portal/migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./database/src/db/portal/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./database/src/db/portal/datasource/index.ts migration:revert"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
